{
  "name": "fcdns",
  "version": "0.3.4",
  "description": "DNS relay server with fact-checking.",
  "keywords": [
    "dns"
  ],
  "bin": "lib/cli.js",
  "files": [
    "lib"
  ],
  "repository": "git@github.com:BlackGlory/fcdns.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src",
    "test": "jest --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --coverage --config jest.config.js",
    "clean": "rimraf lib",
    "build": "run-s build:*",
    "build:compile": "tsc --project tsconfig.build.json",
    "build:patch": "tscpaths --project tsconfig.build.json --src ./src --out ./lib",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint build test",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/jest": "^27.4.1",
    "@types/ms": "^0.7.31",
    "@types/node": "14",
    "@types/ping": "^0.4.1",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "eslint": "^8.10.0",
    "husky": "4",
    "internet-number": "^3.0.1",
    "jest": "^27.5.1",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^27.1.3",
    "tsconfig-paths": "^3.12.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.5.5"
  },
  "dependencies": {
    "@blackglory/errors": "^2.2.0",
    "@blackglory/go": "^0.1.1",
    "@blackglory/types": "^0.6.5",
    "address-range": "^0.2.9",
    "chalk": "^4.1.2",
    "commander": "^9.0.0",
    "extra-filesystem": "^0.4.1",
    "extra-logger": "^0.6.2",
    "extra-promise": "^0.21.2",
    "iterable-operator": "^0.14.5",
    "ms": "^2.1.3",
    "native-node-dns": "^0.7.6",
    "ping": "^0.4.1",
    "return-style": "^0.12.6"
  }
}
