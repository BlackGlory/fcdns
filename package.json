{
  "name": "fcdns",
  "version": "0.6.0",
  "description": "DNS relay server with fact-checking.",
  "keywords": [
    "dns"
  ],
  "type": "module",
  "bin": "lib/cli.js",
  "files": [
    "migrations",
    "lib"
  ],
  "engines": {
    "node": ">=16"
  },
  "repository": "git@github.com:BlackGlory/fcdns.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "prepare": "ts-patch install -s",
    "deduplicate": "yarn-deduplicate",
    "postinstall": "patch-package",
    "prepublishOnly": "run-s postinstall prepare clean build",
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src __tests__",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --runInBand --config jest.config.cjs",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --runInBand --coverage --config jest.config.cjs",
    "test:debug": "cross-env NODE_OPTIONS='--experimental-vm-modules --inspect-brk' jest --runInBand --config jest.config.cjs",
    "clean": "rimraf lib",
    "build": "run-p build:*",
    "build:src": "tsc --project tsconfig.build.json",
    "build:scripts": "tsc --project tsconfig.scripts.json",
    "smoke": "node lib/cli.js --help",
    "release": "standard-version",
    "script:generate-whitelist-by-cc": "ts-node --esm scripts/generate-whitelist-by-cc.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s postinstall prepare clean lint build test smoke",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@blackglory/jest-resolver": "^0.3.0",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/better-sqlite3": "^7.6.3",
    "@types/jest": "^29.4.0",
    "@types/node": "14",
    "@types/ping": "^0.4.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.36.0",
    "extra-prompts": "^0.1.5",
    "husky": "4",
    "internet-number": "^3.0.3",
    "jest": "^29.5.0",
    "jest-resolve": "^29.5.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.5.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-patch": "^2.1.0",
    "typescript": "4.8",
    "typescript-transform-paths": "^3.4.6",
    "webpack": "^5.76.1",
    "webpack-cli": "^5.0.1",
    "webpack-shebang-plugin": "^1.1.8",
    "yarn-deduplicate": "^6.0.1"
  },
  "dependencies": {
    "@blackglory/better-sqlite3-migrations": "^0.1.16",
    "@blackglory/errors": "^3.0.0",
    "@blackglory/prelude": "^0.3.1",
    "@blackglory/structures": "^0.13.1",
    "address-range": "^0.2.12",
    "better-sqlite3": "^8.2.0",
    "chalk": "^5.2.0",
    "commander": "^10.0.0",
    "extra-filesystem": "^0.4.10",
    "extra-lazy": "^2.0.0",
    "extra-logger": "^0.6.9",
    "extra-promise": "^6.0.3",
    "extra-utils": "^5.1.0",
    "iterable-operator": "^4.0.3",
    "migration-files": "^0.4.0",
    "native-node-dns": "0.7.6",
    "native-node-dns-packet": "0.1.5",
    "patch-package": "^6.5.1",
    "ping": "^0.4.4",
    "return-style": "^2.0.3",
    "you-died": "^0.4.3"
  }
}
