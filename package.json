{
  "name": "fcdns",
  "version": "0.1.2",
  "description": "DNS relay server with fact-checking.",
  "keywords": [
    "dns"
  ],
  "bin": "lib/cli.js",
  "files": [
    "lib"
  ],
  "repository": "git@github.com:BlackGlory/fcdns.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src",
    "test": "jest --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --coverage --config jest.config.js",
    "clean": "rimraf lib",
    "build": "run-s build:*",
    "build:compile": "tsc --project tsconfig.build.json",
    "build:patch": "tscpaths --project tsconfig.build.json --src ./src --out ./lib",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint build",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "engines": {
    "node": ">= 14.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@types/fs-extra": "^9.0.11",
    "@types/ms": "^0.7.31",
    "@types/node": "^15.0.2",
    "@types/ping": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.22.1",
    "@typescript-eslint/parser": "^4.22.1",
    "eslint": "^7.26.0",
    "husky": "4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.0",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.2.4"
  },
  "dependencies": {
    "@blackglory/errors": "^1.1.2",
    "@blackglory/go": "^0.1.0",
    "@blackglory/types": "^0.4.2",
    "address-range": "^0.2.2",
    "async-constructor": "^0.4.13",
    "chalk": "^4.1.1",
    "commander": "^7.2.0",
    "extra-logger": "^0.5.4",
    "extra-promise": "^0.16.4",
    "fs-extra": "^10.0.0",
    "iterable-operator": "^0.13.6",
    "ms": "^2.1.3",
    "native-dns": "^0.7.0",
    "ping": "^0.4.1",
    "return-style": "^0.12.3"
  },
  "optionalDependencies": {
    "internet-number": "^2.2.10",
    "tmp-promise": "^3.0.2"
  }
}
