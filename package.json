{
  "name": "fcdns",
  "version": "0.2.5",
  "description": "DNS relay server with fact-checking.",
  "keywords": [
    "dns"
  ],
  "bin": "lib/cli.js",
  "files": [
    "lib"
  ],
  "repository": "git@github.com:BlackGlory/fcdns.git",
  "author": "BlackGlory <woshenmedoubuzhidao@blackglory.me>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint --ext .js,.jsx,.ts,.tsx --quiet src",
    "test": "jest --config jest.config.js",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "test:coverage": "jest --coverage --config jest.config.js",
    "clean": "rimraf lib",
    "build": "run-s build:*",
    "build:compile": "tsc --project tsconfig.build.json",
    "build:patch": "tscpaths --project tsconfig.build.json --src ./src --out ./lib",
    "release": "standard-version"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint build",
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS"
    }
  },
  "engines": {
    "node": ">= 14.5.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.2.1",
    "@commitlint/config-conventional": "^13.2.0",
    "@types/ms": "^0.7.31",
    "@types/node": "^16.11.1",
    "@types/ping": "^0.2.1",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "husky": "4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.1",
    "tsconfig-paths": "^3.11.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@blackglory/errors": "^1.1.2",
    "@blackglory/go": "^0.1.0",
    "@blackglory/types": "^0.5.1",
    "address-range": "^0.2.7",
    "async-constructor": "^0.4.17",
    "chalk": "^4.1.2",
    "commander": "^8.2.0",
    "extra-filesystem": "^0.3.11",
    "extra-logger": "^0.5.5",
    "extra-promise": "^0.19.1",
    "iterable-operator": "^0.14.4",
    "ms": "^2.1.3",
    "native-dns": "^0.7.0",
    "ping": "^0.4.1",
    "return-style": "^0.12.4"
  },
  "optionalDependencies": {
    "internet-number": "^2.2.15"
  }
}
